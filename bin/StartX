#! /bin/bash

#		StartX
#Chequea si el proceso está en ejecución. Si está en ejecución 
#tira error y si no está en ejecución, lo corre.

proceso=$1

if ps ax | grep -v grep | grep $proceso > /dev/null
then
	echo "Error: El proceso $proceso ya se está ejecutando."
	exit 1
else
	"$proceso" &
fi
exit 0
